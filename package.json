    "prompts": "^2.4.2",
    "reflect-metadata": "^0.1.13",
    "tsx": "^4.19.4",
    "typeorm": "^0.3.17",
    "zod": "^3.25.49"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "@types/prompt": "^1.1.9",
    "i": "^0.3.7",
    "nodemon": "^3.0.2",
    "npm": "^11.4.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "node -r tsconfig-paths/register -r ts-node/register ./src/index.ts",
    "ext": "ts, js"
  }
