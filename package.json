  },
  "dependencies": {
    "@langchain/community": "^0.3.45",
    "@langchain/core": "^0.3.57",
    "@langchain/langgraph": "^0.3.0",
    "@langchain/ollama": "^0.2.0",
    "@types/axios": "^0.14.4",
    "@types/multer": "^1.4.11",
    "@types/nodemailer": "^6.4.14",
    "aws-sdk": "^2.1550.0",
    "axios": "^1.9.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.10",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "gigachat": "^0.0.14",
    "jsonwebtoken": "^9.0.2",
    "langchain-gigachat": "^0.0.13",
    "multer": "^1.4.5-lts.1",
